{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es5",
      "es6",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": [
    "src"
  ],
  "noImplicitAny": false,
  "outDir": "lib",
  "sourceMap": true,
  "typeRoots": [
    "node_modules/@types",
    "node_modules/@types/theme-ui__components",
    "./src/types/index.d.ts"
  ],
  "jsx": "react"
}
